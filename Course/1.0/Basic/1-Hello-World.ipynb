    "import tensorflow as tf\n",
    "\n",
    "# 确保 当前 TF 运行环境为静态模式\n",
    "tf.compat.v1.disable_eager_execution()\n",
    "\n",
    "# 定义常量，将其作为一个静态结点加入默认计算图中\n",
    "hello = tf.constant(\"Hello World\")\n",
    "\n",
    "# 定义会话\n",
    "session = tf.compat.v1.Session()\n",
    "\n",
    "# 输出\n",
    "print(session.run(hello))"
   ]
  }
